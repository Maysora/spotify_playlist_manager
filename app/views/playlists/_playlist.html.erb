<% multi_playlist_id = @multi_playlist_ids[playlist.id] %>
<tr>
  <td>
    <%= image_tag playlist.images.first.url, alt: 'playlist image', class: 'playlist-image' if playlist.images.first %>
  </td>
  <td>
    <%= playlist.id %>
  </td>
  <td>
    <%= link_to playlist.name, playlist_tracks_path(playlist_id: playlist) %>
  </td>
  <td>
    <%= playlist.tracks_count %>
  </td>
  <td>
    <%= playlist.owner.id %>
  </td>
  <td>
    <p>
      Collaborative: <%= playlist.collaborative %>
    </p>
    <p>
      Public: <%= playlist.public %>
    </p>
    <p>
      Multi playlist: <%= multi_playlist_id.present? %>
    </p>
  </td>
  <td>
    <%= link_to 'Edit', edit_multi_playlist_path(multi_playlist_id) if multi_playlist_id.present? %>
  </td>
</tr>
